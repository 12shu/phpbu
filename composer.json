{
    "name": "phpbu/phpbu",
    "description": "PHP Backup utility.",
    "type": "library",
    "keywords": [
        "backup", "mysql", "tar", "cloud", "rsync", "sftp"
    ],
    "homepage": "http://phpbu.de/",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Sebastian Feldmann",
            "email": "sebastian@phpbu.de",
            "role": "lead"
        }
    ],
    "support": {
        "issues": "https://github.com/sebastianfeldmann/phpbu/issues"
    },

    "autoload": {
        "psr-4": {
            "phpbu\\App\\": "src/"
        }
    },
    "require": {
        "ext-dom": "*",
        "ext-json": "*",
        "ext-spl": "*",
        "barracuda/copy": "1.1.*",
        "dropbox/dropbox-sdk": "1.1.*",
        "phpseclib/phpseclib": "2.0.*@dev",
        "phpunit/php-timer": "~1.0.2",
        "sebastian/environment": "~1.1",
        "sebastian/version": "~1.0",
        "swiftmailer/swiftmailer": "5.3.*",
        "aws/aws-sdk-php": "2.7.*",
        "softlayer/objectstorage": "dev-master"
    },
    "bin": [
        "phpbu"
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "1.1.x-dev"
        }
    }
}
