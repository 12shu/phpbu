{
  "type": "mysqldump",
  "option": {
    "pathToMysqldump": "/path/to/your/mysql/bin",
    "host": "myHost",
    "user": "myUserName",
    "password": "mySecretPassword",
    "databases": "myDB1,mydB2",
    "tables": "myTable1,myTable2,myTable3",
    "ignoreTables": "myTableToIgnore1,myTableToIgnore2",
    "structureOnly": "myTableWithoutData1,myTableWithoutData2",
    "quick": true,
    "lockTables": true,
    "singleTransaction": true,
    "compress": true,
    "extendedInsert": true,
    "hexBlob": true
  }
}
